<!DOCTYPE html>
<html>
<head>
  <title>User Data</title>
  <link rel="stylesheet" type="text/css" href="../css/customers.css">
  
</head>
<body>

  <h1>User Data</h1>
  <table>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Username</th>
      <th>email</th>
      <th>Last Name</th>
      <th>Phone</th>
    </tr>
    <% users.forEach(user => { %>
      <tr>
      <td><%= user.firstname%></td>
      <td><%= user.lastname%></td>
      <td><%= user.username %></td>
      <td><%= user.password %></td>
      <td><%= user.mail%></td>
      <td><%= user.phone%></td>
    </tr>
    <%  }); %>
    

    
  
 
 

    
  


    

  </table>

</body>
</html>
