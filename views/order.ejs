<!DOCTYPE html>
<html>
<head>
  <title>Order Details</title>
</head>
<body>
  <h1>Order Details</h1>
  <table>
    <thead>
      <tr>
        <th>Order ID</th>
        <th>Price</th>
        <th>Quantity</th>
        <th>Title</th>
        <th>Date</th>
      </tr>
    </thead>
    <tbody>
      <% result.forEach((order) => { %>
      <tr>
        <td><%= order._id %></td>
        <td><%= order.totalPrice %></td>
        <td><%= order.quantity %></td>
        <td><%= order.tripId.title %></td>
        <td><%= order.createdAt.toDateString() %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</body>
</html>
